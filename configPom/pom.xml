<project xmlns="http://maven.apache.org/POM/4.0.0"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

		<modelVersion>4.0.0</modelVersion>

		<groupId>lla.privat.moonserver</groupId>
		<artifactId>configPom</artifactId>
		<version>1.0-SNAPSHOT</version>

		<packaging>pom</packaging>
		<name>LLA Config Pom</name>

		<prerequisites>
				<maven>3.0.2</maven>
		</prerequisites>

		<properties>
				<!-- Dependency Versions -->
				<derby.version>10.12.1.1</derby.version>
				<eclipselink.version>2.6.4</eclipselink.version>
				<guice.version>4.0</guice.version>
				<hibernate.version>4.3.8.Final</hibernate.version>
				<jackson.version>1.9.13</jackson.version>
				<javax-persistence.version>2.1.1</javax-persistence.version>
				<jersey-client.version>1.19</jersey-client.version>
				<jersey-server.version>1.19</jersey-server.version>
				<jersey.version>2.23.2</jersey.version>
				<jackson.fasterxml.version>2.8.3</jackson.fasterxml.version>
				<jsonassert.version>1.3.0</jsonassert.version>
				<!-- <jetty.version>7.6.3.v20120416</jetty.version> -->
				<!-- <jetty.version>7.6.9.v20130131</jetty.version> -->
				<jetty.version>7.6.13.v20130916</jetty.version>
				<!-- <jetty.version>[7.6.16.v20140903, 7.99.0-!]</jetty.version> -->
				<jodatime.version>2.9.4</jodatime.version>
				<junit.version>4.12</junit.version>
				<log4j.version>1.2.17</log4j.version>
				<mockito.version>1.10.8</mockito.version>
				<openjpa.version>2.3.0</openjpa.version>
				<slf4j.version>1.7.21</slf4j.version>
				<!-- <wicket.version>[6.18.0, 6.99.0-!]</wicket.version> -->
				<wicket.version>6.19.0</wicket.version>
				<oval.version>1.85</oval.version>
				<junithamcrest.version>1.3</junithamcrest.version>
				<commons.version>3.3.2</commons.version>
				<commons-lang3.version>3.4</commons-lang3.version>
				<guava.version>19.0</guava.version>
				<httpclient.version>4.3.1</httpclient.version>
				<hibernate-jpamodelgen.version>5.2.4.Final</hibernate-jpamodelgen.version>

				<!-- Plugin Versions -->
				<build-helper-plugin.version>1.9.1</build-helper-plugin.version>
				<deploy-plugin.version>2.8.1</deploy-plugin.version>
				<install-plugin.version>2.5.2</install-plugin.version>
				<compiler-plugin.version>3.8.0</compiler-plugin.version>
				<eclipse-plugin.version>2.10</eclipse-plugin.version>
				<jar-plugin.version>3.0.2</jar-plugin.version>
				<war-plugin.version>3.0.0</war-plugin.version>
				<assembly-plugin.version>2.6</assembly-plugin.version>
				<processor-plugin.version>3.1.0</processor-plugin.version>
				<clean-plugin.version>3.0.0</clean-plugin.version>
				<site-plugin.version>3.5.1</site-plugin.version>
				<surefire-plugin.version>2.21.0</surefire-plugin.version>
				<failsafe-plugin.version>2.21.0</failsafe-plugin.version>
				<resources-plugin.version>3.0.1</resources-plugin.version>

				<!-- system and other versions -->
				<java.version>1.8</java.version>
				<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
				<project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
				<!-- allowed values: R7, 1.0, 1.5, 2.0 or none -->
				<wtp.version>none</wtp.version>

				<!-- DB -->
				<morphia.version>1.1.0</morphia.version>

		</properties>

		<repositories>
				<repository>
						<id>Apache Nexus</id>
						<url>https://repository.apache.org/content/repositories/snapshots/</url>
						<releases>
								<enabled>false</enabled>
						</releases>
						<snapshots>
								<enabled>true</enabled>
						</snapshots>
				</repository>
		</repositories>

		<pluginRepositories>
				<pluginRepository>
						<id>maven-annotation-plugin</id>
						<url>
        http://maven-annotation-plugin.googlecode.com/svn/trunk/mavenrepo
     </url>
				</pluginRepository>
		</pluginRepositories>

</project>
